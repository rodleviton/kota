{% set subheading = page.header.kota.subheading %}

{% block content %}
    <a id="map" href="https://www.google.com/maps/place/Kids+on+the+Avenue+Children's+Centre/@-37.75275,144.965787,18z/data=!4m5!3m4!1s0x0:0x811d51024b4482fd!8m2!3d-37.75275!4d144.9657871?hl=en-US" target="_blank"></a>
    <div class="contact container section">
        <h2 class="{{ subheading ? 'no-margin' : '' }}">{{ page.title }}</h2>
        {% if subheading %}
            <h3>{{ subheading }}</h3>
        {% endif %}
        <div class="content">
            {{ page.content }}
        </div>
    </div>
{% endblock %}